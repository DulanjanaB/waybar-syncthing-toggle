{
  "custom/syncthing": {
    "exec": "~/.config/waybar/scripts/waybar-syncthing-toggle.sh status",
    "return-type": "json",
    "interval": 3,
    "on-click": "~/.config/waybar/scripts/waybar-syncthing-toggle.sh toggle",
    "on-click-middle": "~/.config/waybar/scripts/waybar-syncthing-toggle.sh webui",
    "on-click-right": "~/.config/waybar/scripts/waybar-syncthing-toggle.sh rescan",
    "signal": 9
  }
}
